        "@nextui-org/react": "^2.1.13",
        "@prisma/client": "^5.5.0",
        "@radix-ui/react-label": "^2.0.2",
        "@radix-ui/react-toast": "^1.1.5",
        "@tailwindcss/typography": "^0.5.10",
        "@types/bcrypt": "^5.0.1",
        "autoprefixer": "^10.4.16",
        "bcrypt": "^5.1.1",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "dotenv": "^16.3.1",
        "framer-motion": "^10.16.4",
        "knex": "^3.0.1",
        "lucide-react": "^0.289.0",
        "next": "13.5.6",
        "next-auth": "^4.24.3",
