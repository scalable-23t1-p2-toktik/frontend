        "react-dom": "^18",
        "react-dropzone": "^14.2.3",
        "react-hook-form": "^7.47.0",
        "react-icons": "^4.11.0",
        "react-modal": "^3.16.1",
        "tailwind-merge": "^1.14.0",
        "tailwindcss": "^3.3.5",
        "tailwindcss-animate": "^1.0.7",
        "tailwindcss-animatecss": "^3.0.4",
        "zod": "^3.22.4"
    },
