{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "prisma-generate": "prisma generate",
        "start:prod": "prisma migrate deploy && npm start",
        "lint": "next lint",
        "db:migrate": "set TS_NODE_COMPILER_OPTIONS={\"module\":\"commonjs\"} && knex migrate:latest",
        "db:migrate:undo": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' && knex migrate:down",
        "db:migrate:make": "set TS_NODE_COMPILER_OPTIONS={ \"module\": \"commonjs\" } && knex migrate:make"
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.436.0",
        "@aws-sdk/s3-request-presigner": "^3.436.0",
